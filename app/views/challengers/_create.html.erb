<%= form_for :challenger do |f| %>
  <p>
    <%= f.label :text, "Напишите о себе и мы свяжемся с вами" %><br />
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.label :name, "Телефон для обратной связи" %><br />
    <%= f.text_field :name %>
  </p>

  <%= f.submit "Отправить", :id => "submit" %>
<% end %>
