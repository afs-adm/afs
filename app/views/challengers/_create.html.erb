<%= form_for :challenger do |f| %>
  <p>
    <%= f.label :name, "Ваше имя" %><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :text, "Комментарий" %><br />
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.label :text, "Комментарий" %><br />
    <%= f.text_area :text %>
  </p>

  <%= f.submit "Оставить заявку" %>
<% end %>
